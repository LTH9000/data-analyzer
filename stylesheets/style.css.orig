body, html {
  font-family: sans-serif;
  width: 100%;
  background: url('halftone/halftone.png')
}

html {
  display: table;
  margin: auto;
}

body {
  display: table-cell;
  vertical-align: middle;
}

#overlay {
  position:absolute;
}

header {
  padding: 1em 0px;
  width: 100%;
  left: 0;
  margin-left: 0;
  margin-top: 0;
  background: url('footer_lodyas/footer_lodyas.png');
  color:white;
  text-shadow: 0px 0px 10px black;
  box-shadow: 0px 0px 10px black;
  clear:both;
}

#title {
  font-weight: bold;
  font-size: 2em;
  padding-left: 1em;
}

#content{
  width: 800px;
  margin: auto; /* centers the contents including the slider and the graph */
  margin-top: 20px;
  padding-bottom: 25px;
  clear: both;
}

#selection {
  fill: lightblue;
  fill-opacity: 0.3;
}
svg * {
  stroke:black;
  stroke-width:1;
  fill-opacity: 0.0;
}

#chart-key { /* key for chart shape */
  font-weight: bold;
  border-radius: 5px;
  border: 3px solid #000;
  font-size: 20px;
  width: 100%;
  margin: 40px 0;
  padding-top: 10px;
}

#chart-key-header {
  text-align: center;
  width: 100%;
  margin: auto;
}

